<mxGraphModel>
  <root>
    <!-- 根节点与默认层 -->
    <mxCell id="0"/>
    <mxCell id="1" parent="0"/>

    <!-- 核心组件定义（颜色编码与形状语义） -->
    <!-- mxGraph -->
    <mxCell id="2" value="mxGraph" style="shape=rectangle;rounded=1;strokeColor=#2E75B6;fillColor=#2E75B6;fontColor=white;fontSize=14;" vertex="1" parent="1">
      <mxGeometry x="100" y="50" width="120" height="40" as="geometry"/>
    </mxCell>

    <!-- mxGraphModel -->
    <mxCell id="3" value="mxGraphModel" style="shape=ellipse;strokeColor=#548235;fillColor=#548235;fontColor=white;fontSize=14;" vertex="1" parent="1">
      <mxGeometry x="300" y="50" width="120" height="40" as="geometry"/>
    </mxCell>

    <!-- mxCell -->
    <mxCell id="4" value="mxCell" style="shape=hexagon;strokeColor=#C55A11;fillColor=#C55A11;fontColor=white;fontSize=14;" vertex="1" parent="1">
      <mxGeometry x="500" y="50" width="120" height="40" as="geometry"/>
    </mxCell>

    <!-- 调用关系连线 -->
    <!-- mxGraph → mxGraphModel -->
    <mxCell id="5" style="edgeStyle=orthogonalEdgeStyle;rounded=0;strokeColor=#808080;strokeWidth=2;dashed=1;" edge="1" parent="1" source="2" target="3">
      <mxGeometry relative="1" as="geometry"/>
    </mxCell>

    <!-- mxGraphModel → mxCell -->
    <mxCell id="6" style="edgeStyle=orthogonalEdgeStyle;rounded=0;strokeColor=#808080;strokeWidth=2;dashed=1;" edge="1" parent="1" source="3" target="4">
      <mxGeometry relative="1" as="geometry"/>
    </mxCell>

    <!-- 其他组件（示例） -->
    <mxCell id="7" value="mxCellRenderer" style="shape=roundRectangle;strokeColor=#4472C4;fillColor=#4472C4;fontColor=white;fontSize=14;" vertex="1" parent="1">
      <mxGeometry x="100" y="150" width="120" height="40" as="geometry"/>
    </mxCell>

    <!-- 连线示例 -->
    <mxCell id="8" style="edgeStyle=orthogonalEdgeStyle;rounded=0;strokeColor=#FF0000;strokeWidth=2;" edge="1" parent="1" source="2" target="7">
      <mxGeometry relative="1" as="geometry"/>
    </mxCell>
  </root>
</mxGraphModel>